{
  "id": "BAEC6142-29F9-4BB2-A808-B6432199DB3C",
  "name": "PythonToolInstaller",
  "friendlyName": "ms-resource:loc.friendlyName",
  "description": "ms-resource:loc.description",
  "helpMarkDown": "ms-resource:loc.helpMarkDown",
  "category": "Tool",
  "runsOn": [
    "Agent",
    "DeploymentGroup"
  ],
  "author": "Microsoft Corporation",
  "version": {
    "Major": 0,
    "Minor": 131,
    "Patch": 0
  },
  "preview": true,
  "demands": [],
  "groups": [],
  "instanceNameFormat": "ms-resource:loc.instanceNameFormat",
  "inputs": [
    {
      "name": "versionSpec",
      "type": "string",
      "label": "ms-resource:loc.input.label.versionSpec",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.versionSpec"
    },
    {
      "name": "architectureOption",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.architectureOption",
      "options": {
        "x64": "x64",
        "x86": "x86"
      },
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.architectureOption"
    },
    {
      "name": "installationSource",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.installationSource",
      "required": true,
      "options": {
        "AzureStorage": "Azure storage",
        "FilePath": "File path",
        "Url": "URL"
      },
      "helpMarkDown": "ms-resource:loc.input.help.installationSource"
    },
    {
      "name": "compressedFile",
      "type": "filePath",
      "label": "ms-resource:loc.input.label.compressedFile",
      "required": true,
      "visibleRule": "installationSource == FilePath",
      "helpMarkDown": "ms-resource:loc.input.help.compressedFile"
    },
    {
      "name": "azureResourceManagerEndpoint",
      "type": "connectedService:AzureRM",
      "label": "ms-resource:loc.input.label.azureResourceManagerEndpoint",
      "required": true,
      "visibleRule": "installationSource == AzureStorage",
      "helpMarkDown": "ms-resource:loc.input.help.azureResourceManagerEndpoint"
    },
    {
      "name": "azureStorageAccountName",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.azureStorageAccountName",
      "required": true,
      "properties": {
        "EditableOptions": "True"
      },
      "visibleRule": "installationSource == AzureStorage",
      "helpMarkDown": "ms-resource:loc.input.help.azureStorageAccountName"
    },
    {
      "name": "azureContainerName",
      "type": "string",
      "label": "ms-resource:loc.input.label.azureContainerName",
      "required": true,
      "visibleRule": "installationSource == AzureStorage",
      "helpMarkDown": "ms-resource:loc.input.help.azureContainerName"
    },
    {
      "name": "azureCommonVirtualFile",
      "type": "string",
      "label": "ms-resource:loc.input.label.azureCommonVirtualFile",
      "required": true,
      "visibleRule": "installationSource == AzureStorage",
      "helpMarkDown": "ms-resource:loc.input.help.azureCommonVirtualFile"
    },
    {
      "name": "url",
      "type": "string",
      "label": "ms-resource:loc.input.label.url",
      "required": true,
      "visibleRule": "installationSource == Url",
      "helpMarkDown": "ms-resource:loc.input.help.url"
    },
    {
      "name": "destinationDirectory",
      "type": "filePath",
      "label": "ms-resource:loc.input.label.destinationDirectory",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.destinationDirectory"
    },
    {
      "name": "cleanDestinationDirectory",
      "type": "boolean",
      "label": "ms-resource:loc.input.label.cleanDestinationDirectory",
      "required": true,
      "defaultValue": "true",
      "helpMarkDown": "ms-resource:loc.input.help.cleanDestinationDirectory"
    },
    {
      "name": "outputVariable",
      "type": "string",
      "label": "ms-resource:loc.input.label.outputVariable",
      "required": true,
      "defaultValue": "Python.$(versionSpec)",
      "helpMarkDown": "ms-resource:loc.input.help.outputVariable"
    }
  ],
  "dataSourceBindings": [
    {
      "endpointId": "$(ConnectedServiceNameARM)",
      "target": "storageAccountName",
      "dataSourceName": "AzureStorageAccountRMandClassic"
    }
  ],
  "execution": {
    "Node": {
      "target": "pythontoolinstaller.js",
      "argumentFormat": ""
    }
  },
  "messages": {
    "CleanDestDir": "ms-resource:loc.messages.CleanDestDir",
    "DownloadFromAzureBlobStorage": "ms-resource:loc.messages.DownloadFromAzureBlobStorage",
    "InstallationSourceUnknown": "ms-resource:loc.messages.InstallationSourceUnknown",
    "RetrievingPythonFromAzure": "ms-resource:loc.messages.RetrievingPythonFromAzure",
    "RetrievingPythonFromFilePath": "ms-resource:loc.messages.RetrievingPythonFromFilePath",
    "RetrievingPythonFromUrl": "ms-resource:loc.messages.RetrievingPythonFromUrl",
    "StorageAccountDoesNotExist": "ms-resource:loc.messages.StorageAccountDoesNotExist",
    "SucceedMsg": "ms-resource:loc.messages.SucceedMsg",
    "SuccessFullyFetchedItemList": "ms-resource:loc.messages.SuccessFullyFetchedItemList",
    "UrlNoFile": "ms-resource:loc.messages.UrlNoFile",
    "UrlNoHost": "ms-resource:loc.messages.UrlNoHost",
    "UrlNoProtocol": "ms-resource:loc.messages.UrlNoProtocol"
  }
}