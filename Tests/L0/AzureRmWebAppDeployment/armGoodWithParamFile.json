{
    "which": {
        "msdeploypath\\msdeploy.exe": "msdeploy_path"
    },
    "stats": {
    	"webAppPkg.zip": {
    		"isFile": true
    	}
    },
     "checkPath": {
        "msdeploy_path" : true
    },
    "exec": {
    	"msdeploy_path -verb:sync -source:package='webAppPkg.zip' -dest:auto,ComputerName='https://mytestappKuduUrl/msdeploy.axd?site=mytestapp',UserName='$mytestapp',Password='mytestappPwd',AuthType='Basic' -setParam:name='IIS Web Application Name',value='mytestapp' -enableRule:DoNotDeleteRule -userAgent:TFS_useragent" : {
    		"code": 0,
    		"stdout": "MS Deploy Executed Successfully"
    	},
    	"msdeploy_path -verb:getParameters -source:package='webAppPkg.zip'" : {
    		"code": 1,
    		"stdout": "MS Deploy execSync returned some xml file"
    	}

    },
    "exist": {
    	"webAppPkg.zip": true
    },
    "getVariable": {
    	"ENDPOINT_AUTH_AzureRMSpn": "{\"parameters\":{\"serviceprincipalid\":\"spId\",\"serviceprincipalkey\":\"spKey\",\"tenantid\":\"tenant\"},\"scheme\":\"ServicePrincipal\"}",
   		"ENDPOINT_DATA_AzureRMSpn_SUBSCRIPTIONNAME": "sName", 
    	"ENDPOINT_DATA_AzureRMSpn_SUBSCRIPTIONID": "sId",
    	"SYSTEM_DEFAULTWORKINGDIRECTORY" : "defaultWorkingDirectory",
    	"AZURE_HTTP_USER_AGENT" : "TFS_useragent"
    }
}